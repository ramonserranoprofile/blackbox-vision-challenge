{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "./src/assets/images/favicon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/splash.png"
    },
    "web": {
      "output": "server",
      "bundler": "metro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.quizboxmobile3"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": ""
        }
      ]
    ],
    "name": "QuizBoxMobile3",
    "slug": "QuizBoxMobile3"
  }
}
